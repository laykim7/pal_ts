//  display.getWidget("F1_00_line").setPropertyValue("background_color", red);
  display.getWidget("Label_EVF002").setPropertyValue("background_color", red);
  display.getWidget("Label_EVF003").setPropertyValue("background_color", blue);
	
}

if(devN == "F3"){
  pvs[1].setValue("01");
  display.getWidget("F1"+btnArray[1].toString()).setPropertyValue("border_color", red);
//  display.getWidget("F1_01_line").setPropertyValue("background_color", red);
  display.getWidget("Label_EVF002").setPropertyValue("background_color", blue);
  display.getWidget("Label_EVF003").setPropertyValue("background_color", red);
}

