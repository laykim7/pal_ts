var green = ColorFontUtil.getColorFromRGB(0,150,0);
var blue1 = ColorFontUtil.getColorFromRGB(150,200,255);
var blue = ColorFontUtil.getColorFromRGB(60,150,255);

var btnArray = ["_01","_02", "_03", "_04", "_05", "_06", "_07", "_08", "_09", "_10", "_11", "_12", "_13","_14", "_15", "_16", "_17", "_18", "_19", "_20", "_21", "_22", "_23", "_24"];


var src = PVUtil.getString(pvs[0]);
var strArray = src.split("_");

var devN = strArray[0];
var portN = strArray[1];

var value1;
var value2;

if     (portN == 1 )  {value1 = "00"; value2 = 0; }
else if(portN == 2 )  {value1 = "01"; value2 = 1; }
else if(portN == 3 )  {value1 = "02"; value2 = 2; }
else if(portN == 4 )  {value1 = "03"; value2 = 3; }
else if(portN == 5 )  {value1 = "04"; value2 = 4; }
else if(portN == 6 )  {value1 = "05"; value2 = 5; }
else if(portN == 7 )  {value1 = "06"; value2 = 6; }
else if(portN == 8 )  {value1 = "07"; value2 = 7; }
else if(portN == 9 )  {value1 = "08"; value2 = 8; }
else if(portN == 10)  {value1 = "09"; value2 = 9; }
else if(portN == 11)  {value1 = "10"; value2 = 10;}
else if(portN == 12)  {value1 = "11"; value2 = 11;}
else if(portN == 13)  {value1 = "00"; value2 = 12;}
else if(portN == 14)  {value1 = "01"; value2 = 13;}
else if(portN == 15)  {value1 = "02"; value2 = 14;}
else if(portN == 16)  {value1 = "03"; value2 = 15;}
else if(portN == 17)  {value1 = "04"; value2 = 16;}
else if(portN == 18)  {value1 = "05"; value2 = 17;}
else if(portN == 19)  {value1 = "06"; value2 = 18;}
else if(portN == 20)  {value1 = "07"; value2 = 19;}
else if(portN == 21)  {value1 = "08"; value2 = 20;}
else if(portN == 22)  {value1 = "09"; value2 = 21;}
else if(portN == 23)  {value1 = "10"; value2 = 22;}
else if(portN == 24)  {value1 = "11"; value2 = 23;}
else {value1 = "00"; value2 = 0;}

//display.getWidget("F1_00_line").setPropertyValue("background_color", blue);
//display.getWidget("F1_01_line").setPropertyValue("background_color", blue);

var i = 0;
for(i=0; i< 24; i++)
  display.getWidget("F1"+btnArray[i].toString()).setPropertyValue("border_color", blue);
  
for(i=0; i< 24; i++)
  display.getWidget("F2"+btnArray[i].toString()).setPropertyValue("border_color", blue);
  
for(i=0; i< 24; i++)
  display.getWidget("F3"+btnArray[i].toString()).setPropertyValue("border_color", blue);


var offset=0;
if     (devN == "F1") offset=0;
else if(devN == "F2") offset=3;
else if(devN == "F3") offset=6;
else offset=0;

//pvs[1]   : evf001 sh
