    pvs[3].setValue("SH");
  }
  else{
    pvs[3].setValue("SL");
  }
}
else{
  pvs[3].setValue("SH");
  if(portN < 13 ){
    pvs[3+offset].setValue("SH");
  }
  else{
    pvs[3+offset].setValue("SL");
  }
}

if(devN == "F1"){
	if(portN == 1){
	  display.getWidget("Label_EVF002").setPropertyValue("background_color", red);
	  display.getWidget("Label_EVF003").setPropertyValue("background_color", blue);
